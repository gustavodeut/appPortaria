<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appportaria"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.appportaria.ActLogin"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.appportaria.ActPrincipal"
            android:label="@string/title_activity_act_principal" >
        </activity>
        <activity
            android:name="com.example.appportaria.ActPesquisaFuncionarioVisitante"
            android:label="@string/title_activity_act_pesquisa_funcionario_visitante" >
        </activity>
        <activity
            android:name="com.example.appportaria.ActPesquisaVeiculo"
            android:label="@string/title_activity_act_pesquisa_veiculo" >
        </activity>
        <activity
            android:name="com.example.appportaria.ActPesquisaDestino"
            android:label="@string/title_activity_act_pesquisa_destino" >
        </activity>
        <activity
            android:name="com.example.appportaria.ActPesquisaEntrada"
            android:label="@string/title_activity_act_pesquisa_entrada" >
        </activity>
        <activity
            android:name="com.example.appportaria.ActTeste"
            android:label="@string/title_activity_act_teste" >
        </activity>
    </application>

</manifest>
